{
  "name": "java-api-contacts",
  "version": "0.0.0-development",
  "description": "The project has a simple Java project structure with the following packages (in **src/main/java**)",
  "main": "commitlint.config.js",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "git-commit-msg-linter": "^4.1.2",
    "maven": "^5.0.0",
    "mvn": "^3.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.2"
  },
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kalitch/java-api-contacts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kalitch/java-api-contacts/issues"
  },
  "homepage": "https://github.com/Kalitch/java-api-contacts#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
